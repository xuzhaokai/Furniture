<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.onlinefurnitureshop.management.model.Department"  table="Department">
		<id name="departmentId" type="java.lang.Integer">
		    <column name="departmentId" />
		    <generator class="native" />
     </id>
     <property name="departmentName"></property>
     <property name="departmentdsp" column="departmentDescription"></property>
	</class>

</hibernate-mapping>